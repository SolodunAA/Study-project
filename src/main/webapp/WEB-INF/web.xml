<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>diary.app.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>diary.app.servlet.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>trainingsTypesInteractions</servlet-name>
        <servlet-class>diary.app.servlet.TrainingsTypesInteractionsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>trainingsTypesInteractions</servlet-name>
        <url-pattern>/trainings_types</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>oneTrainingInteractions</servlet-name>
        <servlet-class>diary.app.servlet.OneTrainingInteractionsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>oneTrainingInteractions</servlet-name>
        <url-pattern>/training</url-pattern>
    </servlet-mapping>




    <servlet>
        <servlet-name>getAllTrainings</servlet-name>
        <servlet-class>diary.app.servlet.GetAllTrainingsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getAllTrainings</servlet-name>
        <url-pattern>/training/get/all_trainings</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getTrainingsFromThePeriod</servlet-name>
        <servlet-class>diary.app.servlet.GetTrainingsFromThePeriodServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getTrainingsFromThePeriod</servlet-name>
        <url-pattern>/training/get/training_from_period</url-pattern>
    </servlet-mapping>

</web-app>
